<h1>Signup!</h1>
<%= form_for :user, url: '/users'  do |f| %>

  UserName: <%= f.text_field :username, :required => true %><br/>
  Password Confirmation: <%= f.password_field :password_confirmation,:required => true %><br/>
  Password: <%= f.password_field :password, :required => true %><br/>
  <%= f.submit "Submit" %>

<% end %>
